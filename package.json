{
  "name": "react-combo-select",
  "version": "0.4.8",
  "keywords": "react, select, singleselect, multiselect, search, react-component",
  "description": "React select component that can be used as a single select, multiselect, search",
  "main": "./lib/ComboSelect.jsx",
  "scripts": {
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js  --hot --progress --colors",
    "prepublish": "mkdir lib && ./node_modules/.bin/babel Components/ComboSelect.jsx --out-file lib/ComboSelect.jsx && ./node_modules/.bin/babel Components/ComboSelectItem.jsx --out-file lib/ComboSelectItem.jsx",
    "test": "mocha -r mock-local-storage --compilers js:babel-core/register --require ./test/test.jsx \"test/**/*@(.js|.jsx)\"",
    "postpublish": "rm -rf ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markokostovski/react-combo-select.git"
  },
  "author": "Marko Kostovski <marko.kostovski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markokostovski/react-combo-select/issues"
  },
  "homepage": "https://github.com/markokostovski/react-combo-select#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.2.3",
    "immutable": "^3.7.6",
    "jsdom": "^8.3.0",
    "mocha": "^2.4.5",
    "mock-local-storage": "^1.0.2",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
